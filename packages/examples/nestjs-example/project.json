{
  "name": "examples-nestjs-example",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/examples/nestjs-example/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/packages/examples/nestjs-example",
        "main": "packages/examples/nestjs-example/src/main.ts",
        "tsConfig": "packages/examples/nestjs-example/tsconfig.app.json",
        "assets": ["packages/examples/nestjs-example/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "packages/examples/nestjs-example/src/environments/environment.ts",
              "with": "packages/examples/nestjs-example/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "examples-nestjs-example:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "examples-nestjs-example:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/examples/nestjs-example/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/examples/nestjs-example"],
      "options": {
        "jestConfig": "packages/examples/nestjs-example/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
