{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/basic-example-1/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/basic-example-1",
        "main": "packages/basic-example-1/src/main.ts",
        "tsConfig": "packages/basic-example-1/tsconfig.app.json",
        "assets": ["packages/basic-example-1/src/assets"]
      },
      "configurations": {
        "dev": {
          "optimization": false,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "basic-example-1:build",
        "watch": false
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/basic-example-1/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/basic-example-1"],
      "options": {
        "jestConfig": "packages/basic-example-1/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
